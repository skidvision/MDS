       RowBox[{"bstar", ",", 
        RowBox[{"Tolerance", "\[Rule]", "0"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"ColumnDrop", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Transpose", "[", "u", "]"}], ".", 
          RowBox[{"DiagonalMatrix", "[", 
           RowBox[{"Sqrt", "[", "\[Gamma]", "]"}], "]"}]}], ",", 
         RowBox[{"-", "1"}]}], "]"}], ",", "\[Gamma]"}], "}"}]}]}], 

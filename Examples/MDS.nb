Cell[BoxData[
 RowBox[{
  RowBox[{"AddConstantsFunction", "[", "m_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"d", ",", "parts", ",", "lists"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"d", "=", 
      RowBox[{
       RowBox[{"Dimensions", "[", "m", "]"}], "\[LeftDoubleBracket]", "1", 
       "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"parts", "=", 
      RowBox[{"Flatten", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"a", "+", "1"}], ",", 
              RowBox[{"(", 
               RowBox[{"a", "+", "b", "+", "2"}], ")"}]}], "}"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"a", "+", "b", "+", "2"}], ")"}], ",", 
              RowBox[{"(", 
               RowBox[{"a", "+", "b", "+", "c", "+", "3"}], ")"}]}], "}"}], 
            ",", "\[IndentingNewLine]", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"a", "+", "1"}], ",", 
              RowBox[{"(", 
               RowBox[{"a", "+", "b", "+", "c", "+", "3"}], ")"}]}], "}"}]}], 
           "}"}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"a", ",", "0", ",", 
            RowBox[{"d", "-", "3"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"b", ",", "0", ",", 
            RowBox[{"d", "-", "a", "-", "3"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"c", ",", "0", ",", 
            RowBox[{"d", "-", "a", "-", "b", "-", "3"}]}], "}"}]}], "]"}], 
        ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"lists", "=", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Sort", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"m", "\[LeftDoubleBracket]", 
              RowBox[{
               RowBox[{"#", "\[LeftDoubleBracket]", 
                RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
               RowBox[{"#", "\[LeftDoubleBracket]", 
                RowBox[{"1", ",", "2"}], "\[RightDoubleBracket]"}]}], 
              "\[RightDoubleBracket]"}], ",", 
             RowBox[{"m", "\[LeftDoubleBracket]", 
              RowBox[{
               RowBox[{"#", "\[LeftDoubleBracket]", 
                RowBox[{"2", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
               RowBox[{"#", "\[LeftDoubleBracket]", 
                RowBox[{"2", ",", "2"}], "\[RightDoubleBracket]"}]}], 
              "\[RightDoubleBracket]"}], ",", 
             RowBox[{"m", "\[LeftDoubleBracket]", 
              RowBox[{
               RowBox[{"#", "\[LeftDoubleBracket]", 
                RowBox[{"3", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
               RowBox[{"#", "\[LeftDoubleBracket]", 
                RowBox[{"3", ",", "2"}], "\[RightDoubleBracket]"}]}], 
              "\[RightDoubleBracket]"}]}], "}"}], ",", "Greater"}], "]"}], 
         "&"}], ",", "parts"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Max", "[", 
      RowBox[{"Append", "[", 
       RowBox[{
        RowBox[{"{", "0", "}"}], ",", 
        RowBox[{"Map", "[", 
         RowBox[{"SubtractList", ",", "lists"}], "]"}]}], "]"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",ExpressionUUID->"1e287f19-2f4a-\
42a9-9bac-67915a05aba1"],


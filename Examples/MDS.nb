     RowBox[{"parts", "=", 
      RowBox[{"Flatten", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"a", "+", "1"}], ",", 
              RowBox[{"(", 
               RowBox[{"a", "+", "b", "+", "2"}], ")"}]}], "}"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"a", "+", "b", "+", "2"}], ")"}], ",", 
              RowBox[{"(", 
               RowBox[{"a", "+", "b", "+", "c", "+", "3"}], ")"}]}], "}"}], 
            ",", "\[IndentingNewLine]", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"a", "+", "1"}], ",", 
              RowBox[{"(", 
               RowBox[{"a", "+", "b", "+", "c", "+", "3"}], ")"}]}], "}"}]}], 
           "}"}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"a", ",", "0", ",", 
            RowBox[{"d", "-", "3"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"b", ",", "0", ",", 
            RowBox[{"d", "-", "a", "-", "3"}]}], "}"}], ",", 

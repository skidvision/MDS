Cell[BoxData[
 RowBox[{
  RowBox[{"MakeCoordinates", "[", "bstar_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"u", ",", "\[Gamma]", ",", "v"}], "}"}], ",", 
    "\[IndentingNewLine]", 

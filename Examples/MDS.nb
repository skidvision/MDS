Cell[BoxData[
 RowBox[{
  RowBox[{"AddConstantsFunction", "[", "m_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"d", ",", "parts", ",", "lists"}], "}"}], ",", 

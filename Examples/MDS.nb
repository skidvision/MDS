 RowBox[{
  RowBox[{"IterateMDS", "[", 
   RowBox[{"model_", ",", 
    RowBox[{"times_Integer", ":", "1"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"newx", ",", "news"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"newx", "=", 
         RowBox[{"GuttmanTransform", "[", 
          RowBox[{
           RowBox[{
           "model", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
           ",", 
           RowBox[{
           "model", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
          "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"news", "=", 
         RowBox[{"CalcStress", "[", 
          RowBox[{"newx", ",", 
           RowBox[{
           "model", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
          "]"}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", "times", "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"newx", ",", 
       RowBox[{
       "model", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], ",", 
       RowBox[{
       "model", "\[LeftDoubleBracket]", "3", "\[RightDoubleBracket]"}], ",", 
       "news"}], "}"}]}]}], "]"}]}]], "Input",
 InitializationCell->

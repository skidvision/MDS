Cell["This is a double-centering", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"MakeBstar", "[", "m_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"m2", ",", "means", ",", "mmeans", ",", "n"}], "}"}], ",", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{
       RowBox[{"Dimensions", "[", "m", "]"}], "\[LeftDoubleBracket]", "1", 
       "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"m2", "=", 
      SuperscriptBox["m", "2"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"means", "=", 
      RowBox[{"Map", "[", 
       RowBox[{"Mean", ",", "m2"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"mmeans", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"means", ",", 
        RowBox[{"{", "n", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"m2", "-", "mmeans", "-", 
          RowBox[{"Transpose", "[", "mmeans", "]"}]}], ")"}], "+", 
        RowBox[{"Mean", "[", "means", "]"}]}], ")"}], "/", 
      RowBox[{"-", "2"}]}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True],

Cell[TextData[{
